ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = NO
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/BearSkill" "$PODS_CONFIGURATION_BUILD_DIR/CRAnimation" "$PODS_CONFIGURATION_BUILD_DIR/FLAnimatedImage" "$PODS_CONFIGURATION_BUILD_DIR/YLGIFImage"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/BearSkill/BearSkill.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/CRAnimation/CRAnimation.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/FLAnimatedImage/FLAnimatedImage.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/YLGIFImage/YLGIFImage.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "BearSkill" -framework "CRAnimation" -framework "FLAnimatedImage" -framework "YLGIFImage"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
